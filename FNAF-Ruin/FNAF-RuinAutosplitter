{
    Assembly.Load(File.ReadAllBytes("Components/asl-help")).CreateInstance("Basic");
    vars.Helper.GameName = "Five Nights at Freddys: Ruin";
    vars.Helper.AlertLoadless();

    dynamic[,] _settings =
